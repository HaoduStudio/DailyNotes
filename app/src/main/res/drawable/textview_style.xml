<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 点击的效果 -->
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="#662A2B2C" />
            <corners android:radius="9dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <!-- 点击的效果 -->
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#662A2B2C" />
            <corners android:radius="9dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
    <!-- 这是未点击的效果 -->
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="#312A2B2C" />
            <corners android:radius="9dp" />
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp" />
        </shape>
    </item>

</selector>